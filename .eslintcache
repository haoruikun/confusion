r(){\n    return(\n      <>\n        <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\n          <ModalHeader toggle={this.toggleModal}>Submit Comment</ModalHeader>\n          <ModalBody>\n            <LocalForm onSubmit={this.handleSubmit}>\n              <Col>\n                <Label htmlFor=\"rating\">Rating</Label>\n                <Control.select model=\".rating\" id=\"rating\" name=\"rating\" className=\"form-control\">\n                  <option>-- Please Select Your Rating --</option>\n                  <option>5</option>\n                  <option>4</option>\n                  <option>3</option>\n                  <option>2</option>\n                  <option>1</option>\n                </Control.select>\n\n              </Col>\n              <Col className=\"mt-2\">\n                <Label htmlFor=\"name\">Your Name</Label>\n                <Control.text model=\".name\" name=\"name\" className=\"form-control\" placeholder=\"Your name here\" validators={{\n                    minLength: minLength(3), maxLength: maxLength(15)\n                  }}/>\n                <Errors\n                  className=\"text-danger\"\n                  model=\".name\"\n                  show=\"touched\"\n                  messages={{\n                    minLength: \"Must be at least 3 characters long\",\n                    maxLength: \"Must less than or equal to 15 characters\"\n                  }}\n                />\n              </Col>\n              <Col className=\"mt-2\">\n                <Label htmlFor=\"Comment\">Comment</Label>\n                <Control.textarea model=\".comment\" id=\"comment\" name=\"comment\" placeholder=\"Your comment here\" className=\"form-control\" rows=\"6\"/>\n              </Col>\n              <Col className=\"mt-2\">\n                <Button type=\"submit\" color=\"primary\">Submit</Button>\n              </Col>\n            </LocalForm>\n          </ModalBody>\n        </Modal>\n        <Button onClick={this.toggleModal} outline color=\"primary\">\n          <span className=\"fa fa-comment fa-lg\"></span>\n          { } Submit Comment\n        </Button>\n      </>\n    );\n  }\n}\n\n  function RenderDish({dish}) {\n    return(\n      <Card className=\"col-12 col-md-5 m-1\">\n        <CardImg width=\"100%\" src={baseUrl + dish.image} alt={dish.name} />\n        <CardBody>\n          <CardTitle>{dish.name}</CardTitle>\n          <CardText>{dish.description}</CardText>\n        </CardBody>\n      </Card>\n    )\n  }\n\n  function RenderComments({comments, addComment, dishId}) {\n    var comm = comments.map((com) => {\n      return (\n        <li key={com.id}>\n          <div>{com.comment}</div>\n          <small>-- {com.author}, {new Intl.DateTimeFormat('en-US', { year: 'numeric', month: 'short', day: '2-digit'}).format(new Date(Date.parse(com.date)))}</small>\n        </li>\n      )\n    })\n\n    return (\n      <div className=\"col-12 col-md-5 m-1\">\n        <h4>Comments</h4>\n        <ul class=\"list-unstyled\">\n          {comm}\n        </ul>\n        <CommentForm dishId={dishId} addComment={addComment} />\n      </div>\n    )\n  }\n\n  const DishDetail = (props) => {\n    if (props.isLoading) {\n      return(\n        <div className=\"container\">\n          <div className=\"row\">\n            <Loading />\n          </div>\n        </div>\n      );\n    } else if (props.errMess) {\n      return(\n        <div className=\"container\">\n          <div className=\"row\">\n            <h4>{props.errMess}</h4>\n          </div>\n        </div>\n      );\n    } else if (props.dish != null) {\n      return (\n        <div className=\"container\">\n          <div className=\"row\">\n            <Breadcrumb>\n              <BreadcrumbItem><Link to='/home'>Home</Link></BreadcrumbItem>\n              <BreadcrumbItem><Link to='/menu'>Menu</Link></BreadcrumbItem>\n              <BreadcrumbItem active>{props.dish.name}</BreadcrumbItem>\n            </Breadcrumb>\n            <div className=\"col-12\">\n              <h3>{props.dish.name}</h3>\n              <hr />\n            </div>\n          </div>\n          <div className=\"row\">\n            <RenderDish dish={props.dish} />\n            <RenderComments comments={props.comments}\n              addComment={props.addComment}\n              dishId={props.dish.id} />\n          </div>\n        </div>\n      )\n    } else {\n      return (\n        <div></div>\n      )\n    }\n  }\n\n\n\nexport default DishDetail;\n",["108","109"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/reportWebVitals.js",[],["110","111"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/App.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/MainComponent.js",["112"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/index.js",[],["113","114"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/HeaderComponent.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/FooterComponent.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/HomeComponent.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/ContactComponent.js",["115","116","117","118","119","120","121","122","123","124","125","126"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/shared/comments.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/shared/promotions.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/shared/leaders.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/components/AboutComponent.js",[],["127","128"],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/redux/comments.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/redux/ActionTypes.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/redux/ActionCreators.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/redux/promotions.js",[],"/Users/ruikunhao/Desktop/coursera/react/confusion/src/redux/configureStore.j